model Van {
  id          String  @id @default(cuid())
  name        String  @db.VarChar(60)
  price       Int     @db.SmallInt
  description String  @db.VarChar(1024)
  imageUrl    String
  type        VanType

  hostId   String
  userInfo UserInfo @relation(fields: [hostId], references: [userId])
}
