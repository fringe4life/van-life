model UserInfo {
  userId       String        @id @db.VarChar(25)
  moneyAdded   Float         @default(0)
  rented       Rent[]        @relation("host")
  renter       Rent[]        @relation("renter")
  review       Review[]
  transactions Transaction[]
  user         User          @relation(fields: [userId], references: [id])
  vans         Van[]

  @@index([userId])
}
