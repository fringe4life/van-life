model User {
  id            String    @id @db.VarChar(25)
  name          String
  email         String
  emailVerified Boolean   @default(false)
  image         String?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
  sessions      Session[]
  accounts      Account[]
  userInfo      UserInfo?

  @@unique([email])
  @@map("user")
}
