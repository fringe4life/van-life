model User {
  id            String    @id @db.VarChar(25)
  name          String
  email         String    @unique
  emailVerified Boolean   @default(false)
  image         String?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
  userInfo      UserInfo?
  accounts      Account[]
  sessions      Session[]

  @@map("user")
}
